    "udp": {
        "sendBufferSize": 65536,
        "receiveBufferSize": 65536,
        "packetSize": 1024,
        "dataRate": "1Mbps",
        "enableChecksum": true,
        "enableBroadcast": false,
        "enableMulticast": false
    },
    "enableGlobalRouting" : true

