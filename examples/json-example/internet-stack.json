{
    "ipv4": {
        "defaultTtl": 64,
        "ipForward": true,
        "weakEsModel": false,
        "sendIcmpRedirect": true,
        "enableMtuDiscovery": false,
        "fragmentExpirationTimeout": "30s",
        "mtuDiscoverTimeout": "10s",
        "routeCacheTimeout": "300s",
        "enableChecksum": true
    },
    "ipv6": {
        "defaultTtl": 64,
        "ipForward": true,
        "sendIcmpv6Redirect": true,
        "enableMtuDiscovery": false,
        "fragmentExpirationTimeout": "30s",
        "mtuDiscoverTimeout": "10s",
        "routeCacheTimeout": "300s",
