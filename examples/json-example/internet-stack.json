        "disableIcmpv6": false,
        "enableNdCache": true
    },
    "tcp": {
        "sendBufferSize": 65536,
        "receiveBufferSize": 65536,
        "variant": "TcpNewReno",
        "initialCwnd": 1,
        "segmentSize": 536,
